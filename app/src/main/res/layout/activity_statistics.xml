<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:layout_editor_absoluteY="81dp">


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="117dp"
        app:srcCompat="@drawable/graph"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="6dp" />

    <TextView
        android:id="@+id/locationView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Location Recall Success: "
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="247dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="goBack"
        android:text="Main Page"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="429dp" />

    <TextView
        android:id="@+id/phraseView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phrase Recall Success: "
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="298dp" />

    <TextView
        android:id="@+id/overallView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Overall Recall Success: "
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="351dp" />

    <TextView
        android:id="@+id/locationStats"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        tools:layout_editor_absoluteX="271dp"
        tools:layout_editor_absoluteY="246dp" />

    <TextView
        android:id="@+id/phraseStats"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        tools:layout_editor_absoluteX="271dp"
        tools:layout_editor_absoluteY="298dp" />

    <TextView
        android:id="@+id/overallStats"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        tools:layout_editor_absoluteX="271dp"
        tools:layout_editor_absoluteY="351dp" />
</android.support.constraint.ConstraintLayout>